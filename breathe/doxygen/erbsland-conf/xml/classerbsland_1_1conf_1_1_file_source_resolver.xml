      <memberdef kind="function" id="classerbsland_1_1conf_1_1_file_source_resolver_1ac00c1b45ea6f03dfab4a6c9da222e89e" prot="public" static="no" const="no" explicit="no" inline="no" virt="virtual">
        <type>SourceListPtr</type>
